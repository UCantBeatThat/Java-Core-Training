package com.training.models;

public class BankAccount {
	
	private double balance;

	public BankAccount() {
		super();
		this.balance = 3000.0;
	}
	
	public synchronized double deposit(double amount) {
		
		return (this.balance += amount);
	}
	
	public double withdraw(double amount) {
		
		return this.balance += amount;
	}

}
