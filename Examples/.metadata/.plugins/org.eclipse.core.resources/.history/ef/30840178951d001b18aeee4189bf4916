package com.training.demo;

public class CreditCardService {
	
	private CreditCard[] cardList = new CreditCard()[5];
	public static int indexPos = 0;
	
	
	public CreditCardService(CreditCard[] cardList) {
		super();
		this.cardList = cardList;
	}

	public CreditCard[] getCardList() {
		return cardList;
	}

	public void setCardList(CreditCard[] cardList) {
		this.cardList = cardList;
	}

	public boolean addCard(CreditCard card) {
		boolean result = false;
		
		
		this.cardList[this.cardList.length] = card;
		
		return result;
		
		
	}

}
